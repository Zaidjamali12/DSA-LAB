-- Create the EMPLOYEES table
CREATE TABLE EMPLOYEES (
    EmployeeID INT PRIMARY KEY,
    EMPL_NAME VARCHAR(20),
    AGE TINYINT NOT NULL,
    R_ADDRESS VARCHAR(50),  -- Corrected spelling
    SALARY FLOAT NOT NULL
);

-- Insert data into the EMPLOYEES table
INSERT INTO EMPLOYEES (EmployeeID, EMPL_NAME, AGE, R_ADDRESS, SALARY) VALUES
(38, 'MUHAMMAD ZAID', 23, 'NAWABSHAH', 24000),
(37, 'SIKANDER', 20, 'NAWABSHAH', 34509),
(1, 'NAJAF ALI', 21, 'DADO', 3434343),
(26, 'HAIDER ALI', 34, 'MORO', 120000);

SELECT * FROM EMPLOYEES;

SELECT R_ADDRESS, SALARY FROM EMPLOYEES;

SELECT * FROM EMPLOYEES 
WHERE R_ADDRESS = 'NAWABSHAH' AND R_ADDRESS = 'LARKANA';

SELECT EmployeeID, SALARY, R_ADDRESS FROM EMPLOYEES 
WHERE SALARY >= 30000;

SELECT EMPL_NAME, AGE, R_ADDRESS FROM EMPLOYEES 
WHERE AGE <= 27;

SELECT * FROM EMPLOYEES 
WHERE R_ADDRESS != 'LARKANA';

SELECT EMPL_NAME, SALARY, R_ADDRESS FROM EMPLOYEES 
WHERE SALARY >= 65000 OR R_ADDRESS = 'LAHORE';
